module Math {
  export add
  export { sub as subtract }

  let add :: (a: i32, b: i32) -> i32 = {
    a + b
  }

  let sub :: (a: i32, b: i32) -> i32 = {
    a - b
  }
}

open c

let main :: () = {
  let x = Math.add(10, 5);
  let y = Math.sub(10, 5);
  c.printf("%d\n", x);
  c.printf("%d\n", y);
  x + y
}
