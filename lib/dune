(rule
 (target spec.ml)
 (deps ../examples/spec.no)
 (action
  (with-stdout-to spec.ml
   (progn
    (echo "let content = {s|")
    (run cat ../examples/spec.no)
    (echo "|s}")))))

(library
 (name novac)
 (inline_tests)
 (preprocess 
   (pps
     ppx_deriving.show
     ppx_inline_test))
 (libraries easy_logging spectrum))
