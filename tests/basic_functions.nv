let add :: a, b = a + b
let sub :: a: i32, b: i32
    -> i32 = a - b

let do_many_things
:: x, y, z = {
    let res := x + y * 3
    let res := y - res / 2
    let res := res - z + x
    return res
}

let main :: () = {
    @println("add(1, 2) = {}", add(1,2))
    @println("sub(11, 2) = {}", sub(11, 2))
    @println("do_many_things(1, 2, 4) = {}", do_many_things(1, 2, 4))
}
